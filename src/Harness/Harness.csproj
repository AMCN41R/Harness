<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Version>2.0.0</Version>
    <Description>Harness is a .NET library designed to manage the state of one or more Mongo databases during testing.</Description>
    <Company>AMCN41R</Company>
    <Copyright>Copyright Â©  2016</Copyright>
    <PackageProjectUrl>https://github.com/AMCN41R/harness</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/AMCN41R/harness/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/AMCN41R/harness</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard2.0\Harness.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MongoDB.Driver" Version="2.4.3" />
    <PackageReference Include="Newtonsoft.Json" Version="9.0.1" />
  </ItemGroup>

</Project>
